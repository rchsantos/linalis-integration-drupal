name = Linalis Theme
description = A Bootstrap theme customized for this particular project
core = 7.x


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[navigation]             = 'Navigation'
regions[header]                 = 'Top Bar'
regions[jumbotron]              = 'Jumbotron'
regions[content]                = 'Content'
regions[content_img_car]        = 'Content Images Carre'
regions[content_img_cerc]       = 'Content Images Cercle'
